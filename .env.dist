COMPOSE_PROJECT_NAME=storefront
MAGENTO_PATH=~/projects/magento/storefront/repos
DOCKER_PATH=~/projects/magento/storefront/magento-docker
MAGENTO_DOMAIN=magento.test
INSTALLED_REPOS="magento2ce saas-export commerce-data-export"
INSTALLED_REPOS_commerce_export="magento2ce magento2ee commerce-data-export commerce-data-export-ee saas-export storefront-graphql-proxy"
